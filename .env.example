# x402mesh Environment Variables - Example Configuration
# Copy this file to .env and fill in your actual values

# ===================
# Optional: AI Services
# ===================
OPENAI_API_KEY=sk-your-openai-key-here
GEMINI_API_KEY=your-gemini-key-here
REMOVEBG_API_KEY=your-removebg-key-here

# ===================
# Solana Configuration
# ===================
REAL_TRANSACTIONS=true
SOLANA_RPC_URL=https://api.devnet.solana.com

# ===================
# Service Ports
# ===================
REGISTRY_PORT=3001
ROUTER_PORT=3002
REGISTRY_URL=http://localhost:3001
ROUTER_URL=http://localhost:3002

# ===================
# Database (Optional)
# ===================
# DATABASE_URL=postgresql://user:password@localhost:5432/x402mesh

# ===================
# Agent Wallets
# ===================
TRANSLATOR_WALLET=TranslatorWallet
SUMMARIZER_WALLET=SummarizerWallet
ANALYZER_WALLET=AnalyzerWallet
IMAGE_GENERATOR_WALLET=ImageGeneratorWallet
BACKGROUND_REMOVER_WALLET=BackgroundRemoverWallet
